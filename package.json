{
    "name": "zoc-rexx-syntax-highlighting",
    "displayName": "ZOC REXX Syntax Highlighting",
    "description": "Syntax-Highlighting of the REXX language with Extensions for ZOC Terminal",
    "version": "1.0.3",
    "publisher": "zocterminal",
	"icon": "package-icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/zocterminal/zoc-rexx-syntax-highlighting-for-vscode"
	},
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "rexx",
                "aliases": [
                    "REXX",
                    "rexx"
                ],
                "extensions": [
                    ".rex",
                    ".rexx",
                    ".rx",
                    ".rxj",
                    ".zrx"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "rexx",
                "scopeName": "source.rexx",
                "path": "./syntaxes/rexx.tmLanguageNew.json"
            }
        ]
    }
}
