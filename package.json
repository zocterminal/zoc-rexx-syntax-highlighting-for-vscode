{
    "name": "rexx-syntax-highlighting",
    "displayName": "rexx-syntax-highlighting",
    "description": "syntax highlighting of the rexx language",
    "version": "1.0.3",
    "publisher": "zocterminal",
    "repository": "https://github.com/zocterminal/zoc-rexx-syntax-highlighting-for-vscode",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "rexx",
                "aliases": [
                    "REXX",
                    "rexx"
                ],
                "extensions": [
                    ".rex",
                    ".rexx",
                    ".rx",
                    ".rxj",
                    ".zrx"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "rexx",
                "scopeName": "source.rexx",
                "path": "./syntaxes/rexx.tmLanguageNew.json"
            }
        ]
    }
}
